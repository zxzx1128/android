        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".food_list.CalculatorActivity"></activity>
        <activity android:name=".food_grid.FoodDescActivity" />
        <activity android:name=".food_grid.FoodGridActivity" />
        <activity android:name=".food_list.InfoListActivity" />
        <activity android:name=".HomeMenuActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
    </application>

</manifest>